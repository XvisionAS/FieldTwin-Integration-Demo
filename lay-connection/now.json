{
  "version": 2,
  "name": "futureon-LC",
  "builds": [
      { "src": "package.json", "use": "@now/static-build" }
  ],
  "routes": [
    {"src": "^/js/(.*)", "dest": "/js/$1"},
    {"src": "^/css/(.*)", "dest": "/css/$1"},
    {"src": "^/img/(.*)", "dest": "/img/$1"},
    {"src": ".*", "dest": "/index.html"}
  ]
}
